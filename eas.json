    "development": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://univibesbackend.onrender.com/api/v1"
      }
    },
    "production": {
      "node": "18.19.0",
      "android": {
        "buildType": "apk",
